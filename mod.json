{
	"geode": "4.0.0-beta.1",
	"gd": {
		"win": "2.2074",
		"android": "2.2074",
		"mac": "2.2074",
		"ios": "2.2074"
	},
	"version": "v1.0.0",
	"id": "user95401.ryzen",
	"name": "Ryzen - Mods & TPs Place",
	"developer": "user95401",
	"description": "Brings a standalone catalog for any mods and resource packs.",
	"links": {
		"community": "https://discord.gg/6SxAbk3bTA",
		"homepage": "https://t.me/user95401_channel/6",
		"source": "https://github.com/user95401/Ryzen"
	},
	"early-load": true,
	"dependencies": [
		{
			"id": "geode.texture-loader",
			"version": ">=v1.6.0",
			"importance": "suggested"
		}
	],
	"resources": {
		"files": [ "files/*" ],
		"sprites": [ "sprites/*.png", "*.png" ],
		"spritesheets": {
			"rzn_sprites": [ "sprites/*.png", "*.png" ]
		}
	},
	"settings": {
		"rzn_menu_music": {
			"name": "Rzn Menu Music",
			"type": "bool",
			"default": true
		},
		"tpldr_old_look": {
			"name": "TpLdr old look ext.",
			"description": "Make textureldr select layer\nlooks like original mod.\n(just enable it and look if its good for u)",
			"type": "bool",
			"default": true
		},
		"issues_repo": {
			"name": "Issues repo",
			"description": "https://github.com/{issues_repo}",
			"type": "string",
			"filter": "=+_-/0123456789QWERTYUIOPASDFGHJKLZXCVBNMqwertyuiopasdfghjklzxcvbnm",
			"default": "user95401/Ryzen-Pubs"
		},
		"per_page": {
			"name": "Items per page",
			"description": "is about api.github.com/repos/{issues_repo}/issues?<cr>per_page={per_page}</c>...",
			"type": "int",
			"default": 16
		}
	}
}
